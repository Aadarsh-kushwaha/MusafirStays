<!-- <h1>Flash message for success</h1> -->
 <% if(success && success.length){  %>


     
     <div class="alert alert-success alert-dismissible fade show col-8 offset-3" role="alert">
         <%= success %>
         <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
     </div>
   <% } %>